{
  "name": "yanyu",
  "version": "0.0.3-beta",
  "description": "A Chinese speech synthesis and recognition library for nodejs, aiming at high efficiency real time voice casting.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json && npm run clean",
    "start": "node ./index.js",
    "build": "tsc",
    "prepublishOnly": "npm test && npm run build",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "preversion": "npm run lint",
    "clean": "rm -rf ./*.mp3",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": "https://github.com/duckino/yanyu",
  "keywords": [
    "speech",
    "recognition",
    "synthesis",
    "npm",
    "node",
    "Chinese"
  ],
  "author": "duckino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/duckino/yanyu/issues"
  },
  "homepage": "https://github.com/duckino/yanyu#readme",
  "dependencies": {
    "@types/audiosprite": "^0.6.0",
    "@types/node": "^13.13.46",
    "pinyin": "^2.9.1",
    "typescript": "^3.9.9"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/pinyin": "^2.8.2",
    "audiosprite": "^0.7.2",
    "jest": "^25.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^25.5.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "node-notifier": ">=8.0.1"
  }
}
