{
  "name": "yanyu",
  "version": "0.1.0",
  "description": "A Chinese speech synthesis and recognition library for nodejs, aiming at high efficiency real time voice casting.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json && npm run clean",
    "start": "node ./index.js",
    "build": "tsc",
    "prepublishOnly": "npm test && npm run build",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "preversion": "npm run lint",
    "clean": "rm -rf ./*.mp3",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": "https://github.com/byhow/yanyu",
  "keywords": [
    "speech",
    "recognition",
    "synthesis",
    "npm",
    "node",
    "Chinese"
  ],
  "author": "byhow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/byhow/yanyu/issues"
  },
  "homepage": "https://github.com/byhow/yanyu#readme",
  "dependencies": {
    "@types/audiosprite": "^0.6.1",
    "@types/node": "^16.11.1",
    "pinyin": "^2.10.2",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/pinyin": "^2.10.0",
    "audiosprite": "^0.7.2",
    "jest": "^27.3.1",
    "node-notifier": ">=10.0.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}
